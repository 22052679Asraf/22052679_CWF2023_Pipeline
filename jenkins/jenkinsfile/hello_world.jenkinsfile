pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    

 stage('SonarQube analysis') {
       // requires SonarQube Scanner 2.8+
       def scannerHome = tool 'SONAR_RUNNER';
       withSonarQubeEnv('sonarqube') {
            bat "\"${scannerHome}\\bin\\sonar-scanner.bat\""
       }
}
}

}
